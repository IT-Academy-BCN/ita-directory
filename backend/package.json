{
  "name": "be_ita_directory",
  "version": "1.0.0",
  "description": "Backend ITAcademy Directory project (react + node tracks)",
  "main": "server.js",
  "scripts": {
    "dev": "export NODE_ENV=development && nodemon --legacy-watch server.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate-dev": "prisma migrate dev",
    "prisma:seed": "npx prisma db seed --preview-feature",
    "start:dev": "npm run prisma:generate && npm run prisma:migrate-dev && npm run prisma:seed && npm run dev",
    "test": "jest"
  },
  "keywords": [
    "node",
    "sequelize",
    "mysql"
  ],
  "author": "Kevin Mamaqi Kapllani",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.25.0",
    "argon2": "^0.28.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jsdoc-swagger": "^1.6.2",
    "hashids": "^2.2.8",
    "helmet": "^4.6.0",
    "jest": "^27.3.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "socket.io": "^4.1.2",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "ejs": "^3.1.6",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "prisma": "^2.25.0"
  }
}
